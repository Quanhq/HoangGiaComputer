<script>
    $(document).ready(function () {
        getAllCategory()
        getAllProduct()
    })
    function getAllCategory() {
        $.ajax({
            type: 'GET',
            url: '/category/get-all-category',
            data: {},
            success: function (data) {
                $('.box-list-category-sidebar').empty()
                $('.list-menu-category').empty()
                $('.list-menu-category').append(`<li class="active dropdown yamm-fw"><a href="/" data-hover="dropdown" class="dropdown-toggle">Trang Chủ</a></li>`)
                for (let i = 0; i < data.length; i++) {

                    $('.box-list-category-sidebar').append(` <li class="dropdown menu-item"><a href="/category/?category=${data[i].categoryName}"  class="dropdown-toggle" ><i aria-hidden="true"></i>${data[i].categoryName}</a></li>`)
                    if (i < 7) {
                        $('.list-menu-category').append(`<li class="active dropdown yamm-fw"><a href="/category/?category=${data[i].categoryName}" data-hover="dropdown" class="dropdown-toggle">${data[i].categoryName}</a></li>`)
                    }
                }
            },
            error: function (err) {
                console.log(err);
            }
        });
    }
    function getAllProduct() {
        let selectCategory = getDataURL('category') || ''
        $.ajax({
            type: 'GET',
            url: '/all-product-by-category',
            data: { categoryName: selectCategory },
            success: function (data) {
                $('#category-select').html(selectCategory)
                $('#list-product').empty()

                let textPrice = ''
                for (let i = 0; i < data.length; i++) {
                    if (Number(data[i].price) === 0) {
                        textPrice = "Liên Hệ"
                    } else {
                        textPrice = data[i].price
                    }

                    $('#list-product').append(`
                                    <div class="col-sm-6 col-md-4 wow fadeInUp">
                                        <div class="products">
                                            <div class="product">
                                                <div class="product-image">
                                                    <div class="image"><a href="/detail-product/?product=${data[i]._id}" ><img style='height: 250px;' src="/assets/images/product/${data[i].image[0]}" alt=""></a>
                                                    </div>
                                                </div>
                                                <div class="product-info text-left">
                                                    <h3 class="name"><a style="font-weight: bold;" href="/detail-product/?product=${data[i]._id}" >${data[i].productName}</a></h3>
                                                    <div class="rating rateit-small rateit"><button id="rateit-reset-2" data-role="none" class="rateit-reset" aria-label="reset rating" aria-controls="rateit-range-2" style="display: none;"></button><div id="rateit-range-2" class="rateit-range" tabindex="0" role="slider" aria-label="rating" aria-owns="rateit-reset-2" aria-valuemin="0" aria-valuemax="5" aria-valuenow="4" aria-readonly="true" style="width: 70px; height: 14px;"><div class="rateit-selected" style="height: 14px; width: 56px;"></div><div class="rateit-hover" style="height:14px"></div></div></div>
                                                    <div class="product-price"><span class="price">Giá : ${textPrice}</span><span class="price-before-discount"></span> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                `)



                }
            },
            error: function (err) {
                console.log(err);
            }
        });
    }
    function getDataURL(name) {
        if (name = (new RegExp('[?&]' + encodeURIComponent(name) + '=([^&]*)')).exec(location.search))
            return decodeURIComponent(name[1]);
    }
</script>