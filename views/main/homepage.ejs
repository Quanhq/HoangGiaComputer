                }
                for (let j = 0; j < data.length; j++) {
                    if (j < 7) {
                        $('.list-menu-category').append(`<li class="active dropdown yamm-fw"><a href="/category/?category=${data[j].categoryName}" data-hover="dropdown"  >${data[j].categoryName}</a></li>`)
                    }
                }
            },
            error: function (err) {
                console.log(err);
            }
        });
    }

